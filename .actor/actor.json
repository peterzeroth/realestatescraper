{
  "actorSpecification": 1,
  "name": "tangerine_incubator/realestatescraper",
  "title": "Real Estate Listing Scraper",
  "description": "Scrapes real estate listings from various websites",
  "version": "1.0.0",
  "buildTag": "latest",
  "meta": {
    "templateId": "js-start"
  },
  "input": "./input_schema.json",
  "dockerfile": "./Dockerfile",
  "readme": "./README.md",
  "storages": {
    "dataset": {
      "actorSpecification": 1,
      "title": "Real Estate Listings",
      "views": {
        "default": {
          "title": "Listings",
          "transformation": {
            "fields": [
              "address",
              "suburb",
              "state",
              "postcode",
              "fullAddress",
              "propertyStatus",
              "priceText",
              "price",
              "propertyType",
              "bedrooms",
              "bathrooms",
              "parkingSpaces",
              "landSize",
              "buildingSize",
              "url",
              "scrapedAt"
            ]
          }
        }
      }
    }
  }
}

